   --merge \
   --input-files-per-job=9999 \
   --input-dir=root://cmsxrootd.hep.wisc.edu//store/user/tperry/Tbar_s_${version}-MuNuPAT \
   merged_${version}_Tbar_s \
   $CMSSW_BASE

farmoutAnalysisJobs \
   --merge \
   --input-files-per-job=9999 \
   --input-dir=root://cmsxrootd.hep.wisc.edu//store/user/tperry/Tbar_t_${version}-MuNuPAT \
   merged_${version}_Tbar_t \
   $CMSSW_BASE

farmoutAnalysisJobs \
   --merge \
   --input-files-per-job=9999 \
   --input-dir=root://cmsxrootd.hep.wisc.edu//store/user/tperry/Tbar_tW_${version}-MuNuPAT \
   merged_${version}_Tbar_tW \
   $CMSSW_BASE

