   --merge \
   --input-files-per-job=9999 \
   --input-dir=root://cmsxrootd.hep.wisc.edu//store/user/tperry/W3Jet_p2_${version}-MuNuPAT \
   merged_${version}_W3Jet_p2 \
   $CMSSW_BASE

