#!/bin/sh
# Author: T.M.Perry UW-Madison
# Merges MC samples using farmout (uses mergeFiles.C)

voms-proxy-init --voms cms --valid 100:00

version="v5"

farmoutAnalysisJobs \
   --merge \
   --input-files-per-job=9999 \
   --input-dir=root://cmsxrootd.hep.wisc.edu//store/user/tperry/Drell_${version}-MuNuPAT \
   merged_${version}_Drell \
   $CMSSW_BASE

farmoutAnalysisJobs \
   --merge \
   --input-files-per-job=9999 \
   --input-dir=root://cmsxrootd.hep.wisc.edu//store/user/tperry/TTbar_full_${version}-MuNuPAT \
   merged_${version}_TTbar_full \
   $CMSSW_BASE

farmoutAnalysisJobs \
   --merge \
   --input-files-per-job=9999 \
   --input-dir=root://cmsxrootd.hep.wisc.edu//store/user/tperry/TTbar_semi_${version}-MuNuPAT \
   merged_${version}_TTbar_semi \
   $CMSSW_BASE

farmoutAnalysisJobs \
   --merge \
   --input-files-per-job=9999 \
   --input-dir=root://cmsxrootd.hep.wisc.edu//store/user/tperry/T_s_${version}-MuNuPAT \
   merged_${version}_T_s \
   $CMSSW_BASE

farmoutAnalysisJobs \
   --merge \
   --input-files-per-job=9999 \
   --input-dir=root://cmsxrootd.hep.wisc.edu//store/user/tperry/T_t_${version}-MuNuPAT \
   merged_${version}_T_t \
   $CMSSW_BASE

