   --merge \
   --input-files-per-job=9999 \
   --input-dir=root://cmsxrootd.hep.wisc.edu//store/user/tperry/T_tW_${version}-MuNuPAT \
   merged_${version}_T_tW \
   $CMSSW_BASE

