   --merge \
   --input-files-per-job=9999 \
   --input-dir=root://cmsxrootd.hep.wisc.edu//store/user/tperry/W2Jet_josh_${version}-MuNuPAT \
   merged_${version}_W2Jet_josh \
   $CMSSW_BASE

