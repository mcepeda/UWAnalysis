   --merge \
   --input-files-per-job=9999 \
   --input-dir=root://cmsxrootd.hep.wisc.edu//store/user/tperry/W1Jet_josh_${version}-MuNuPAT \
   merged_${version}_W1Jet_josh \
   $CMSSW_BASE

farmoutAnalysisJobs \
   --merge \
   --input-files-per-job=9999 \
   --input-dir=root://cmsxrootd.hep.wisc.edu//store/user/tperry/W1Jet_tapas_${version}-MuNuPAT \
   merged_${version}_W1Jet_tapas \
   $CMSSW_BASE

