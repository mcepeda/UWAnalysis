#!/bin/sh
voms-proxy-init --voms cms --valid 100:00

version="SV_3"

cat MNu-Pat_Data.py  > MuNuPATDat.py
cat CONDOR_DATA.py >> MuNuPATDat.py

#farmoutAnalysisJobs --input-files-per-job=1 --shared-fs --resubmit-failed-jobs --memory-requirements=5000 \
farmoutAnalysisJobs --input-files-per-job=1 --shared-fs --memory-requirements=3000 \
  --input-dir=root://cmsxrootd.hep.wisc.edu//store/user/tapas/SingleMu/Run2012A-22Jan2013-v1/AOD/2013-06-25-8TeV-53X-PatTuple_Master/ \
  ${version}_DATA1_8TeV $CMSSW_BASE $CMSSW_BASE/src/UWAnalysis/CRAB/MuNu/MuNuPATDat.py

#farmoutAnalysisJobs --input-files-per-job=1 --shared-fs --resubmit-failed-jobs --memory-requirements=5000 \
farmoutAnalysisJobs --input-files-per-job=1 --shared-fs --memory-requirements=3000 \
  --input-dir=root://cmsxrootd.hep.wisc.edu//store/user/tapas/SingleMu/Run2012B-22Jan2013-v1/AOD/2013-06-25-8TeV-53X-PatTuple_Master/ \
  ${version}_DATA2_8TeV $CMSSW_BASE $CMSSW_BASE/src/UWAnalysis/CRAB/MuNu/MuNuPATDat.py

#farmoutAnalysisJobs --input-files-per-job=1 --shared-fs --resubmit-failed-jobs --memory-requirements=5000 \
farmoutAnalysisJobs --input-files-per-job=1 --shared-fs --memory-requirements=3000 \
  --input-dir=root://cmsxrootd.hep.wisc.edu//store/user/tapas/SingleMu/Run2012C-22Jan2013-v1/AOD/2013-06-25-8TeV-53X-PatTuple_Master/ \
  ${version}_DATA3_8TeV $CMSSW_BASE $CMSSW_BASE/src/UWAnalysis/CRAB/MuNu/MuNuPATDat.py

#farmoutAnalysisJobs --input-files-per-job=1 --shared-fs --resubmit-failed-jobs --memory-requirements=5000 \
farmoutAnalysisJobs --input-files-per-job=1 --shared-fs --memory-requirements=3000 \
  --input-dir=root://cmsxrootd.hep.wisc.edu//store/user/tapas/SingleMu/Run2012D-22Jan2013-v1/AOD/2013-06-25-8TeV-53X-PatTuple_Master/ \
  ${version}_DATA4_8TeV $CMSSW_BASE $CMSSW_BASE/src/UWAnalysis/CRAB/MuNu/MuNuPATDat.py

rm MuNuPATDat.py
