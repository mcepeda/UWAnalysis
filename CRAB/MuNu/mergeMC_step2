#!/bin/sh
# Author: T.M.Perry UW-Madison
# Moves/Merges files from /hdfs/... to working directory

version="v5"
cd $CMSSW_BASE/src/UWAnalysis/CRAB/MuNu/data/
mkdir ${version}
cd ${version}

scp /hdfs/store/user/$USER/merged_${version}_Drell-mergeFilesJob/*.root .
hadd TTbar.root /hdfs/store/user/$USER/merged_${version}_TTbar_full-mergeFilesJob/*.root /hdfs/store/user/$USER/merged_${version}_TTbar_semi-mergeFilesJob/*.root
scp /hdfs/store/user/$USER/merged_${version}_T_s-mergeFilesJob/*.root .
scp /hdfs/store/user/$USER/merged_${version}_T_t-mergeFilesJob/*.root .
scp /hdfs/store/user/$USER/merged_${version}_T_tW-mergeFilesJob/*.root .
scp /hdfs/store/user/$USER/merged_${version}_Tbar_s-mergeFilesJob/*.root .
scp /hdfs/store/user/$USER/merged_${version}_Tbar_t-mergeFilesJob/*.root .
scp /hdfs/store/user/$USER/merged_${version}_Tbar_tW-mergeFilesJob/*.root .
hadd W1Jet.root /hdfs/store/user/$USER/merged_${version}_W1Jet_josh-mergeFilesJob/*.root /hdfs/store/user/$USER/merged_${version}_W1Jet_tapas-mergeFilesJob/*.root
hadd W2Jet.root /hdfs/store/user/$USER/merged_${version}_W2Jet_josh-mergeFilesJob/*.root /hdfs/store/user/$USER/merged_${version}_W2Jet_tapas-mergeFilesJob/*.root
hadd W3Jet.root /hdfs/store/user/$USER/merged_${version}_W3Jet_p1-mergeFilesJob/*.root /hdfs/store/user/$USER/merged_${version}_W3Jet_p2-mergeFilesJob/*.root
scp /hdfs/store/user/$USER/merged_${version}_W4Jet-mergeFilesJob/*.root .
hadd WJets.root /hdfs/store/user/$USER/merged_${version}_WJets_p1-mergeFilesJob/*.root /hdfs/store/user/$USER/merged_${version}_WJets_p2-mergeFilesJob/*.root
scp /hdfs/store/user/$USER/merged_${version}_WW-mergeFilesJob/*.root .
scp /hdfs/store/user/$USER/merged_${version}_WZ-mergeFilesJob/*.root .
scp /hdfs/store/user/$USER/merged_${version}_ZZ-mergeFilesJob/*.root .
